slice_expansion() {
  no_sel = "nothing selected";
  one[x,1] = "one_x";
  one[:,1] = "one_colon";
  one[x:,1] = "one_x_colon";
  one[:y,1] = "one_colon_y";
  one[x:y,1] = "one_x_colon_y";
  two[p:q,x:y] = "two";

  [f(x), f(y)] first := "1st", second := "2nd";
  [x+y*f(z),1] third;
  fourth := "four";
  return 0;
}
