slice_expansion() {
  no_sel = "nothing selected";
  one[x] = "one_x";
  one[:] = "one_colon";
  one[x:] = "one_x_colon";
  one[:y] = "one_colon_y";
  one[x:y] = "one_x_colon_y";
  two[p:q,x:y] = "two";

  [f(x), f(y)] first := "1st", second := "2nd";
  [x+y*f(z)] third;
  fourth := "four";
  return 0;
}
