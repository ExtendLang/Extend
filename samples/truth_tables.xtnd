calc_or() {
  [3,1] first;
  [1,3] second;

  first[0,0] = 1;
  first[1,0] = 0;
  first[2,0] = empty;

  second[0,0] = printf("Evaluated second\n") -> 1;
  second[0,1] = printf("Evaluated second\n") -> 0;
  second[0,2] = printf("Evaluated second\n") -> empty;

  [3,3] or_table := #first || #second;

  return printf(1, toString(or_table) + "\n");
}

main(args) {
  return calc_or() -> 0;
}
